{
  "name": "like_wanko for chrome extension",
  "version": "0.1",
  "manifest_version": 2,
  "description": "change the love button of Twitter",
  "web_accessible_resources": ["likewanko.css"],
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "css": ["likewanko.css"]
    }
  ],
  "icons": { "128": "wanko.png" }
}
